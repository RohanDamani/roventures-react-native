{
  "expo": {
    "name": "Roventures",
    "description": "View photos, videos, and more from Roventures.",
    "slug": "roventures",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/roventures_icon.png",
    "splash": {
      "image": "./assets/roventures_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.Roventures.Roventures",

    }
  }
}
